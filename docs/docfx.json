{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "exclude": [ "**.Tests.csproj", "**/bin/**", "**/obj/**" ],
          "src": "../src"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**/*.{md,yml}", "*.{md,yml}" ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default" ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© Jeremy Knight. Generated with <strong>DocFX</strong></span>",
      "_appTitle": "JK.Common Documentation",
      "_enableSearch": "true",
      "_gitContribute": {
        "repo": "https://github.com/jeremyknight-me/JK.Common"
      },
      "_gitUrlPattern": "github"
    },
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}